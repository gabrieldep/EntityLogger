<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>EntityLogger</PackageId>
    <Version>1.4</Version>
    <Authors>Gabriel de Paula Silva</Authors>
    <Company>Por minha conta :)</Company>
	<Description>Nuget package to log editing, creating and deleting database entities automatically.</Description>
	<RepositoryType>Git</RepositoryType>
    <RepositoryUrl>https://github.com/gabrieldep/EntityLogger</RepositoryUrl>
    <TargetFramework>net7.0</TargetFramework>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="EntityFramework" Version="6.4.4" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="7.0.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="7.0.1" />
  </ItemGroup>
</Project>
